<?xml version="1.0"?>
<thlogic>
  <attr sel="#articleTitle" th:text="'게시글 상세'" />
  <attr sel="#articleTable" th:object="${article}">
    <attr sel="#title" th:text="*{title} ?: _" />
    <attr sel="#nickname" th:text="*{nickname} ?: _" />
    <attr sel="#content" th:text="*{content} ?: _" />
    <attr sel="#modifyAt" th:text="*{modifyAt} ?: _" />
    <attr sel="#view" th:text="*{view} ?: _" />
  </attr>
  <attr sel="#backToArticles" th:href="@{#}" />
</thlogic>
